{
  "name": "more",
  "version": "1.0.0",
  "description": "MoreOver Real Experience bot",
  "main": "src/main.ts",
  "scripts": {
    "start": "nodemon",
    "start:dev": "ENVIRONMENT=dev nodemon",
    "test": "ENVIRONMENT=test jest",
    "test:more": "yarn test src/more/",
    "test:nbr": "yarn test src/nbr/",
    "coverage": "yarn test --collectCoverage",
    "tdd": "yarn test --watch",
    "format": "prettier-eslint 'src/**/*.{ts,json}' --write",
    "lint": "./node_modules/.bin/eslint **/*.ts"
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn format",
      "git add --all"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dusaniuk/more.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "more"
  ],
  "author": "Yaroslav Dusaniuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dusaniuk/more/issues"
  },
  "homepage": "https://github.com/dusaniuk/more#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "firebase": "^6.3.5",
    "firebase-admin": "^8.3.0",
    "nodemon": "^2.0.1",
    "telegraf": "^3.34.1",
    "telegraf-i18n": "^6.6.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.7.0",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^4.2.1",
    "faker": "^4.1.0",
    "husky": "^3.0.2",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.1",
    "prettier-eslint-cli": "^5.0.0",
    "ts-jest": "^24.1.0"
  }
}
